{
  "_version": "1.12.0",
  "sap.app": {
    "id": "agenda.app",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "Agenda App",
    "description": "Agenda management app with SAPUI5 and CAP"
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "sap-icon://calendar"
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "agenda.view.App",
      "type": "XML",
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.120.0",
      "libs": {
        "sap.m": {}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "agenda.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "bypassed": {
          "target": ["main"]
        }
      },
      "routes": [
        {"pattern": "", "name": "main", "target": "main"},
        {"pattern": "persons", "name": "persons", "target": "persons"},
        {"pattern": "locations", "name": "locations", "target": "locations"},
        {"pattern": "events", "name": "events", "target": "events"},
        {"pattern": "groups", "name": "groups", "target": "groups"}
      ],
      "targets": {
        "main": {"viewName": "Main"},
        "persons": {"viewName": "Persons"},
        "locations": {"viewName": "Locations"},
        "events": {"viewName": "Events"},
        "groups": {"viewName": "Groups"}
      }
    }
  }
}
